{% if social_networks is not null %}
    {% for social_network in social_networks %}
        <li>
            <a class="btn btn-square" target="_blank" href="{{ social_network.link }}"><i class="fa fa-{{ social_network.fa_icon }}"></i><span class="sr-only">{{ social_network.title }}</span></a>
        </li>
    {% endfor %}
{% endif %}